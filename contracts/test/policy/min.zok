import "hashes/sha256/512bitPadded" as sha256;
from "ecc/babyjubjubParams" import BabyJubJubParams;
import "signatures/verifyEddsa.code" as verifyEddsa;
import "ecc/babyjubjubParams.code" as context;

def main(u32 min, private u32[8] name, private u32[8] value, private field[2] R, private field S, field[2] A, u32[8] M0, u32[8] M1) -> bool {
	BabyJubJubParams context = context();
	bool isVerified = verifyEddsa(R, S, A, M0, M1, context);

    u32[8] h = sha256(name, value);

    bool isHash = h == M0;
    bool inRange = min < value[7];

    return isVerified && isHash && inRange;
}
